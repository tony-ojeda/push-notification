{
  "name": "self-service",
  "version": "0.5.0",
  "scripts": {
    "build:dev": "run-s gitversion descriptor:local ngBuild:dev",
    "build:lint": "gulp tslint --errorsAllowed 0 --reportLimit 100",
    "build:pre": "run-s gitversion descriptor:pre ngBuild:pre",
    "build:int": "run-s gitversion descriptor:int ngBuild:int",
    "build:prod": "run-s gitversion descriptor:prod ngBuild:prod",
    "bundle-report": "node --max_old_space_size=8096 node_modules/@angular/cli/bin/ng build --configuration=production client --stats-json && webpack-bundle-analyzer dist/client/stats.json",
    "check:node": "check-node-version --package",
    "constants:pre": "gulp wrConstants --env pre",
    "constants:int": "gulp wrConstants --env int",
    "constants:prod": "gulp wrConstants --env prod",
    "deploy:pre": "run-p devConstants:pre ngBuild:pre",
    "deploy:int": "run-p devConstants:int ngBuild:int",
    "deploy:prod": "run-p devConstants:prod ngBuild:prod",
    "descriptor:local": "gulp getDescriptor --env local",
    "descriptor:pre": "gulp getDescriptor --env pre",
    "descriptor:prod": "gulp getDescriptor --env prod",
    "descriptor:trex": "gulp getDescriptor --env trex",
    "descriptor:int": "gulp getDescriptor --env int",
    "devConstants:pre": "gulp wrDevConstants --env pre",
    "devConstants:int": "gulp wrDevConstants --env int",
    "devConstants:prod": "gulp wrDevConstants --env prod",
    "devConstants:trex": "gulp wrDevConstants --env trex",
    "e2e": "ng e2e",
    "gitAddVersionFiles": "git add apps/client/src/index.html core/shared/src/providers/constants/proxy/git-describe.ts core/shared/src/providers/constants/proxy/git-log.ts",
    "gitInfo": "gulp wrGitInfo",
    "gitversion": "node git-version.js",
    "grunt": "grunt",
    "gulp": "gulp",
    "i": "rm -rf package-lock.json && rm -rf yarn.lock && npm i && rm -rf node_modules/@bit && npm run rm:nodes",
    "lint": "ng lint",
    "memory": "export NODE_OPTIONS='--max-old-space-size=80192'",
    "ng": "ng",
    "ngBuild:dev": "ng build --configuration=dev client",
    "ngBuild:pre": "ng build --configuration=pre client --prod --build-optimizer",
    "ngBuild:int": "ng build --configuration=int client --prod --build-optimizer",
    "ngBuild:prod": "ng build --configuration=production client --prod --build-optimizer",
    "ngServerLocal": "ng serve --open --host 0.0.0.0 client",
    "postinstall": "node node_modules/patternlab-node/core/scripts/postinstall.js",
    "prebuild": "run-p descriptor:pre",
    "intbuild": "run-p descriptor:int",
    "prestart": "npm run check:node",
    "prettier-tslint:ts:all": "prettier-tslint fix './**/*.ts'",
    "prettier-tslint:ts:proxies": "prettier-tslint fix './core/shared/src/providers/**/*.ts'",
    "prettier:check:ts": "prettier --single-quote -c --parser typescript --print-width 120 './**/*.ts'",
    "prettier:js": "prettier --single-quote --parser babel --print-width 120 --write",
    "prettier:scss": "prettier --single-quote --parser css --print-width 120 --write",
    "prettier:ts:all": "prettier --single-quote --parser typescript --print-width 120 --write './**/*.ts'",
    "prettier:ts": "prettier --single-quote --parser typescript --print-width 120 --write",
    "reportLint": "node scripts/tasks.js",
    "rm:nodes": "del-cli ./core/**/node_modules",
    "s:pre": "ng serve --configuration=pre --open --host 0.0.0.0 client",
    "s:int": "ng serve --configuration=int --open --host 0.0.0.0 client",
    "s:prod": "ng serve --configuration=prod --open --host 0.0.0.0 client",
    "sasslint:all": "sass-lint ./source/scss/*.scss --verbose --no-exit",
    "sasslint": "sass-lint --format --verbose --no-exit",
    "search:prod": "npm run prebuild & ng build --configuration=production search",
    "start:dev": "run-p descriptor:prod start start:patternlab",
    "start:integration": "npm run prebuild & ng serve --configuration=integration --open --host 0.0.0.0 client",
    "start:ngrok": "npm run & ng serve --configuration=ngrok --open --host 0.0.0.0 client",
    "start:patternlab": "npm run check:node & grunt patternlab:serve",
    "start:pre": "npm run prebuild & ng serve --configuration=pre --open --host 0.0.0.0 client",
    "start:int": "npm run intbuild & ng serve --configuration=pre --open --host 0.0.0.0 client",
    "start:prod": "npm run descriptor:prod & ng serve --configuration=prod --open --host 0.0.0.0 client",
    "start:search": "npm run prebuild & ng serve --open --host 0.0.0.0 --port 4201 search",
    "start:trex": "npm run descriptor:trex & ng serve --configuration=trex --open --host 0.0.0.0 client",
    "start": "run-s descriptor:local ngServerLocal",
    "test": "ng test",
    "test:client": "ng test client",
    "test:client:generate": "ng test client --watch=false --code-coverage",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "tslint:fix": "tslint -c tslint.json --fix apps/client/src/**/*.ts -t verbose",
    "tslint:report": "node scripts/tasks.js",
    "tslint": "gulp tslint --errorsAllowed 0 --reportLimit 50",
    "tslinter:fix:all": "tslint --format verbose --project tsconfig.json --config tslint.json -t stylish --fix '**/*{.ts,.tsx}' -e './node_modules/**/*' -e './**/node_modules/**/*' -e './src/core/**/index.ts'",
    "tslinter:fix": "tslint --format verbose --project tsconfig.json --config tslint.json -t stylish --fix",
    "version": "run-s gitversion gitInfo gitAddVersionFiles",
    "y": "rm -rf package-lock.json && rm -rf yarn.lock && yarn install && rm -rf node_modules/@bit && npm run rm:nodes",
    "zip:pre": "run-s build:pre zip",
    "zip:int": "run-s build:int zip",
    "zip:prod": "run-s build:prod zip",
    "zip": "gulp zip"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=8.11.1 <9.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "run-s tslint"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "npm run prettier:js",
        "git add"
      ],
      "*.{ts,tsx}": [
        "npm run prettier:ts",
        "npm run tslinter:fix",
        "git add"
      ],
      "*.scss": [
        "npm run prettier:scss",
        "npm run sasslint",
        "git add"
      ]
    },
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "keywords": [
    "Pattern Lab",
    "Atomic Web Design",
    "Node",
    "Grunt",
    "Javascript",
    "Self Service Client"
  ],
  "repository": {
    "type": "git",
    "url": "git://bitbucket.org/mxperu/self_service_client.git"
  },
  "dependencies": {
    "@agm/core": "1.0.0-beta.5",
    "@agm/js-marker-clusterer": "1.0.0-beta.5",
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/fire": "^5.0.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/entity": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@stomp/stompjs": "^5.2.0",
    "@types/jquery": "^3.3.31",
    "@types/jszip": "^3.1.4",
    "core-js": "^2.5.4",
    "file-saver": "^1.3.8",
    "firebase": "^5.5.0",
    "hammerjs": "^2.0.8",
    "image-compressor.js": "^1.1.4",
    "jquery": "^3.3.1",
    "js-marker-clusterer": "1.0.0",
    "jwt-decode": "2.2.0",
    "lodash-es": "^4.17.15",
    "moment": "^2.29.3",
    "net": "^1.0.2",
    "ng-pick-datetime": "6.0.16",
    "ng-pick-datetime-moment": "^1.0.7",
    "ng-recaptcha": "4.2.1",
    "ngrx-store-localstorage": "^7.0.1",
    "ngrx-store-logger": "^0.2.4",
    "ngx-pagination": "3.2.1",
    "ngx-uploader": "^6.0.3",
    "rxjs": "~6.2.1",
    "rxjs-compat": "~6.2.1",
    "sockjs-client": "^1.5.1",
    "stompjs": "^2.3.3",
    "stream": "0.0.2",
    "ts-md5": "^1.2.7",
    "vanilla-text-mask": "5.1.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^6.0.0",
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "7.7.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@ngrx/schematics": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "@ngrx/store-log-monitor": "^3.0.2",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "@types/sockjs-client": "^1.5.1",
    "@types/through": "0.0.29",
    "angular-tslint-rules": "^1.5.0",
    "autoprefixer": "^8.6.4",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.1.1",
    "bs-html-injector": "^3.0.0",
    "chalk": "^2.4.1",
    "check-node-version": "^3.2.0",
    "codelyzer": "4.2.1",
    "computer-name": "^0.1.0",
    "del-cli": "^2.0.0",
    "git-describe": "^4.0.3",
    "git-log-to-json": "^1.1.1",
    "grunt": "~1.0.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-postcss": "^0.9.0",
    "grunt-sass-lint": "^0.2.4",
    "gulp": "^4.0.2",
    "gulp-exec": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^2.0.1",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-run": "^1.7.1",
    "gulp-tslint": "^8.1.3",
    "gulp-util": "^3.0.8",
    "gulp-yaml": "^2.0.4",
    "gulp-zip": "^5.0.1",
    "handlebars": "^4.5.0",
    "husky": "^1.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jszip": "^3.9.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.2.1",
    "lodash": "^4.17.15",
    "log-symbols": "^2.2.0",
    "minimist": "^1.2.0",
    "nanoreset": "^3.0.0",
    "ngrx-store-freeze": "^0.2.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.3",
    "patternlab-node": "^2.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-scss": "^1.0.6",
    "prettier": "1.18.2",
    "prettier-tslint": "0.4.2",
    "protractor": "^5.4.2",
    "require-dir": "^1.2.0",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sonar-scanner": "^3.1.0",
    "source-map-explorer": "^2.1.1",
    "styleguidekit-assets-default": "^3.0.0",
    "styleguidekit-mustache-default": "^3.0.0",
    "stylelint": "9.3.0",
    "stylelint-config-standard": "18.2.0",
    "through": "^2.3.8",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-consistent-codestyle": "1.13.3",
    "tslint-html-report": "^2.0.3",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.1.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "yargs": "^14.2.0"
  }
}
